.panel.primary
  .panel-header
    %h1
      = "#{professor.first_name} #{professor.last_name}"
      - if current_user.present? && current_user.access_rights?(4)
        = link_to edit_professor_path(professor.id) do
          %button.edit Edit
  .panel-body
    %p= professor.description.html_safe
    %p
      %b rating:
      = professor.rating

